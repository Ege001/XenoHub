Tab:AddButton({
    Title = "MoneyGun",
    Locked = false,
    Callback = function()
        local rep = game:GetService("ReplicatedStorage")
        local Players = game:GetService("Players")
        local localPlayer = Players.LocalPlayer
        local itemName = "MoneyGun"

        local function tryNotify(title, text)
            pcall(function()
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = title,
                    Text = text,
                    Duration = 4
                })
            end)
        end

        local ok, err = pcall(function()
            -- Prefer an explicit GetItem remote if it exists (several possible spellings)
            local re = rep:FindFirstChild("RE") or rep:FindFirstChild("Remotes") or rep
            local candidates = {"GetItem", "Getƒ∞tem", "Get_Item", "GetTool", "GetToolToServer"}
            for _, name in ipairs(candidates) do
                local r = re:FindFirstChild(name)
                if r then
                    if r.InvokeServer then
                        r:InvokeServer(itemName)
                        tryNotify("MoneyGun", "Requested via "..name.." (InvokeServer).")
                        return
                    elseif r.FireServer then
                        r:FireServer(itemName)
                        tryNotify("MoneyGun", "Requested via "..name.." (FireServer).")
                        return
                    end
                end
            end

            -- Fallback: use the same trigger used by other items in this script
            local trigger = re:FindFirstChild("1Playe1rTrigge1rEven1t") or rep:FindFirstChild("1Playe1rTrigge1rEven1t")
            if trigger then
                local args = {
                    [1] = "AcceptedToolToServer",
                    [2] = itemName,
                    [3] = localPlayer
                }
                if trigger.FireServer then
                    trigger:FireServer(unpack(args))
                    tryNotify("MoneyGun", "Requested via 1Playe1rTrigge1rEven1t (FireServer).")
                    return
                elseif trigger.InvokeServer then
                    trigger:InvokeServer(unpack(args))
                    tryNotify("MoneyGun", "Requested via 1Playe1rTrigge1rEven1t (InvokeServer).")
                    return
                end
            end

            -- Very last resort: try a generic Remotes/GetItem on top-level Remotes folder
            local topRemotes = rep:FindFirstChild("Remotes")
            if topRemotes then
                local r = topRemotes:FindFirstChild("GetItem") or topRemotes:FindFirstChild("Getƒ∞tem")
                if r and r.FireServer then
                    r:FireServer(itemName)
                    tryNotify("MoneyGun", "Requested via Remotes/GetItem (FireServer).")
                    return
                end
            end

            -- If nothing worked, notify user
            tryNotify("MoneyGun", "Hayƒ±r suitable remote found to request MoneyGun.")
        end)

        if not ok then
            tryNotify("MoneyGun", "Error requesting MoneyGun: " .. tostring(err))
        end
    end
})

