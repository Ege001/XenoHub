            selectedPlayer = nil
            selectedPlayerName = nil
            dropdownValue = ""
            if killDropdown.SetValue then killDropdown:SetValue("") end
            if viewToggle then viewToggle:Set(false) end
            if stalkToggle then stalkToggle:Set(false) end
            UpdateDropdown()
        end

        if not viewingTarget and viewToggle.CurrentValue then
            viewToggle:Set(false)
        end
        if not stalking and stalkToggle.CurrentValue then
