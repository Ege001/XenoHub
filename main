local function UpdateDropdown()
    local tablePlayers = Players:GetPlayers()
    local newPlayers = {}
    if killDropdown then
        for _, player in ipairs(tablePlayers) do
            if player.Name ~= LocalPlayer.Name then
                table.insert(newPlayers, player.Name)
            end
        end
        killDropdown:Set(newPlayers)

        -- Se√ßili oyuncu halen server'da mƒ± kontrol et
        if selectedPlayerName and not Players:FindFirstChild(selectedPlayerName) then
            -- se√ßili ki≈üi yoksa temizle
            selectedPlayerName = nil
            selectedPlayer = nil
            dropdownValue = ""
            if killDropdown.SetValue then killDropdown:SetValue("") end

            -- toggle'larƒ± kapat
            if viewToggle and viewToggle.CurrentValue then viewToggle:Set(false) end
            if stalkToggle and stalkToggle.CurrentValue then stalkToggle:Set(false) end
        else
            -- eƒüer hala varsa, dropdown'da se√ßili g√∂ster
            if selectedPlayerName and killDropdown.SetValue then
                killDropdown:SetValue(selectedPlayerName)
            end

            -- Eƒüer se√ßili bir oyuncu varsa ve view/stalk toggle'larƒ± a√ßƒ±ksa onlara baƒülan
            if selectedPlayerName and Players:FindFirstChild(selectedPlayerName) then
                local pl = Players:FindFirstChild(selectedPlayerName)
                if viewToggle and viewToggle.CurrentValue then
                    startView(pl)
                end
                if stalkToggle and stalkToggle.CurrentValue then
                    startStalk(pl)
                end
            end
        end
    end
end

