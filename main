                                    [6] = 0.25,
                                },
                                [12] = true,
                                [13] = false,
                            }))
                        end)
                    end)
                    
                    task.spawn(function()
                        pcall(function()
                            GuinRemote:FireServer(table.unpack({
                                [1] = hrp,
                                [2] = hrp,
                                [3] = Vector3.new(0, -1, 0),
                                [4] = Vector3.new(0, 0, 0),
